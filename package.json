  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabriciopashaj/as-malloc/issues"
  },
  "homepage": "https://github.com/fabriciopashaj/as-malloc#readme",
  "dependencies": {
    "@assemblyscript/loader": "^0.19.1"
  },
  "devDependencies": {
    "assemblyscript": "^0.19.1"
  }
}
