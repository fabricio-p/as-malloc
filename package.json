{
  "name": "as-malloc",
  "version": "0.0.1",
  "description": "Malloc in AssemblyScript",
  "main": "index.js",
  "scripts": {
    "test": "node tests/index.js",
    "asbuild:untouched": "asc assembly/index.ts --importMemory --target debug",
    "asbuild:optimized": "asc assembly/index.ts --importMemory --target release",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized"
